{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "removeComments": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  },
  "include": ["./src/"],
  "compileOnSave": true
}
